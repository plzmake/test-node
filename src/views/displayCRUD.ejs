<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>displayCRUD</title>
</head>
<style>
    table,
    th,
    td {
        border:  1px solid black;
        border-collapse: collapse;
    }
</style>
<body>
    displayCRUD from view
    <table style="width: 100%;">
        <tr>
            <th>Email</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Address</th>
            <th>Action</th>
            <th>Id</th>
        </tr>
        <% for(var i=0; i < dataTable.length; i++) { %>
            <tr>
              <td><%= dataTable[i].email %></td>
              <td><%= dataTable[i].firstName %></td>
              <td><%= dataTable[i].lastName %></td>
              <td><%= dataTable[i].address %></td>
              
              <td>
                <a href="edit-crud?id=<%= dataTable[i].id%>" class="btn-edit" >Edit</a>
                <a href="delete-crud?id=<%= dataTable[i].id%>" class="btn-edit">Delete</a>
              </td>
              <td><%= dataTable[i].id %></td>
            </tr>
         <% } %>
    </table>
</body>
</html>